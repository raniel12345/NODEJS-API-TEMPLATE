{
  "name": "onclickapi",
  "version": "1.0.0",
  "description": "OnClickAPI",
  "main": "./build/index.js",
  "scripts": {
    "postinstall": "npx tsc",
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/server.ts",
    "prod": "npx pm2 start pm2.yaml",
    "test": "jest --coverage --verbose"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongoose": "^5.7.5",
    "node-uuid": "^1.4.8",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.7",
    "then-request": "^6.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  }
}
